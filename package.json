{
    "name": "v-departure-board",
    "version": "0.0.3",
    "files": [
        "dist"
    ],
    "main": "./dist/v-departure-board.umd.js",
    "module": "./dist/v-departure-board.es.js",
    "exports": {
        ".": {
            "import": "./dist/v-departure-board.es.js",
            "require": "./dist/v-departure-board.umd.js"
        }
    },
    "types": "./dist/types/index.d.ts",
    "private": false,
    "author": "kelvin2go <kelvinho84@gmail.com>",
    "license": "MIT",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/kelvin2go/vue-departure-board.git"
    },
    "keywords": [
        "vue",
        "vue3",
        "component",
        "departure-board",
        "departure",
        "board"
    ],
    "scripts": {
        "dev": "vite",
        "preview": "vite preview",
        "build": "vite build && vue-tsc --emitDeclarationOnly && mv dist/src dist/types",
        "preserve": "vite build",
        "serve": "vite preview --port 5050",
        "typeCheck": "vue-tsc --noEmit"
